{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "align": false,
    "space-before-function-paren": false,
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  },
  "rulesDirectory": []
}
